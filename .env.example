# Global Trade Analytics - Environment Variables Example
# Copy this file to .env and fill in your actual values

# Google Cloud Platform Configuration
GCP_PROJECT_ID=your-project-id-here
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/credentials.json

# GCS Bucket Configuration
# This is set by the update_env.sh script after Terraform runs
GCS_BUCKET_NAME=your-project-id-data-lake

# BigQuery Dataset Names
# These should match the names in your Terraform configuration
BQ_DATASET=raw_trade_data
RAW_DATASET = "raw_trade_data"  # The BigQuery dataset where raw data is stored
COMBINED_DATASET = "combined_trade_data"  # Using the temporary dataset that works correctly
PROCESSED_DATASET = "processed_trade_data"  # The BigQuery dataset for processed data
ANALYTICS_DATASET = "trade_analytics"  # The BigQuery dataset for analytics views


DATA_DIR=data
TEST_DATA_DIR=test_data
MAX_WORKERS=4




